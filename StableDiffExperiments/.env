
# DOTENV settings and their default values
DOTENV_DISABLE_RADAR=1           # Suppresses the Radar info message from dotenv
DOTENV_PRECOMMIT=false           # Shows precommit warning message (set to false to suppress)
DOTENV_KEY=                      # (Optional) Used for Dotenv Vault, not needed for local use
DOTENV_CONFIG_PATH=.env          # Path to your .env file (default is .env in current directory)
DOTENV_CONFIG_ENCODING=utf8      # Encoding for .env file (default: utf8)
DOTENV_CONFIG_DEBUG=false        # If true, enables debug output from dotenv
DOTENV_MEASURE=false             # If true, enables dotenvx performance measurement logs
DOTENV_PARSE_EXPAND=true         # If true, enables variable expansion in .env values
DOTENV_PARSE_JSON=false          # If true, parses .env values as JSON

# Stable Diffusion settings
PROMPT=                         # Main description of the image (must be set)
NEGATIVE_PROMPT=                # Words to avoid (e.g., "blurry", "bad anatomy")
STEPS=30                        # Number of steps for image generation
CFG_SCALE=8.0                   # How strongly the prompt is followed
SAMPLER_NAME=DPM++ 2M Karras    # Which sampler to use
WIDTH=800                       # Image width
HEIGHT=1024                     # Image height
SEED=-1                         # Seed for reproducibility (-1 for random)
BATCH_SIZE=1                    # Images generated per batch
RESTORE_FACES=true              # Fix distorted faces (good for portraits)
TILING=false                    # Enable seamless textures
N_ITER=1                        # Number of batches to generate
